class LPU
{
 void Pmethod(){System.out.println("I am an Educational Institute!");}
 }
 class Teacher extends LPU
 {
 void faculty(){System.out.println("Hey, I am a Faculty of LPU");}
 }
 class Stud extends Teacher
 {
 void student(){System.out.println("Hey, I am a Student of LPU");}
 }
 
  public class Main{
 public static void main(String args[]){
 LPU institute=new LPU();
 institute.Pmethod();
 
 Teacher t=new Teacher();
 t.faculty();
 
 Stud s=new Stud();
 s.student(); }
 }
